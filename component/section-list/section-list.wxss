/* component/label-list/section-list.wxss */
.section-list {
  margin-bottom: .3rem;
}
.section-label {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #ffffff;
  height: 2rem;
  line-height: 2rem;
}
  .label-text {
    position: relative;
    font-size: 11pt;
    font-weight: bolder;
    padding-left: 1.6rem;
  }
    .label-text::before {
      content: "";
      position: absolute;
      top: .7rem;
      left: .8rem;
      display: inline-block;
      width: .5rem;
      height: .6rem;
      background-repeat: no-repeat;
      background-size: 100% 100%;
      background-image: url(data:image/png;base64,);
    }
  .more-text {
    font-size: 9pt;
    color: #888888;
    padding: 0 .8rem;
  }

.row {
  display: flex;
  flex-wrap: wrap;
  background-color: #ffffff;
}
  .col-6 {
    width: 50%;
    flex: 0 1 none;
    box-sizing: border-box;
  }
  .col-6:nth-child(2n+1) {
    padding-right: .15rem;
  }
  .col-6:nth-child(2n) {
    padding-left: .15rem;
  }
.show-count-down {
  display: block;
}
