/**index.wxss**/
.app-logo {
  height: 1.3rem;
  width: 5rem;
}
.tofeet-logo {
  height: 1.3rem;
  width: 1.3rem;
}
.search-input {
  position: relative;
  flex: 1;
  padding: .2rem .5rem .2rem 1.5rem;
  margin: 0 .8rem;
  color: #999;
  border-radius: 0.8vw;
  background: #f2f2f2;
  font-size: 11pt;
}
  .search-input::before {
    content: " ";
    position: absolute;
    top: .5rem;
    left: .4rem;
    width: .8rem;
    height: .8rem;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,);
  }
.swiper-wrapper,
.swiper-wrapper .slide-image {
  width: 100%;
  height: 53.333vw;
}
