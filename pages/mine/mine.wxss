/* pages/mine/mine.wxss */
.user-info-wrapper {
  height: 6rem;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #ffffff;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
}
  .user-info {
    display: flex;
  }
  .user-icon {
    width: 3rem;
    height: 3rem;
    background-color: #ccc;
    margin: 0 .8rem;
    border-radius: 50%;
  }
  .info-detail {
    display: flex;
    flex-direction: column;
  }
    .user-name {
      font-size: 9pt;
    }
.info-link {
  display: flex;
  align-items: center;
  font-size: 9pt;
  background: rgba(0, 51, 51, 0.3);
  border-radius: 1.2rem 0 0 1.2rem;
  padding: .4rem .8rem;
}
  .set-icon {
    width: .8rem;
    height: .8rem;
    margin-right: .4rem;
  }

.row {
  display: flex;
  background: #ffffff;
  margin-bottom: .5rem;
}
  .col-6 {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: .8rem 0;
  }
.page-icon {
  width: 1.5rem;
  height: 1.5rem;
  margin-bottom: .5rem;
}

.form {
  background: #ffffff;
  padding-left: .8rem;
}
  .btn-item {
    height: 2.7rem;
    line-height: 2.7rem;
    border-bottom: 1px solid #ccc;
    font-size: 13pt;
  }
  .form .btn-item:last-child {
    border-bottom: none;
  }

.logout-button {
  background: #f26a5c;
  color: #ffffff;
  margin: 1.5rem .8rem;
}